/dts-v1/;

#include "qcom-ipq4019.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/soc/qcom,tcsr.h>

/ {
	model = "ASUS Lyra Mini MAP-AC1300";
	compatible = "asus,map-ac1300";

	aliases {
		led-boot = &led_blue0;
		led-failsafe = &led_red0;
		led-running = &led_blue0;
		led-upgrade = &led_red0;
	};

	soc {
		mdio@90000 {
			status = "okay";
		};

		ess-psgmii@98000 {
			status = "okay";
		};

		tcsr@1949000 {
			compatible = "qcom,tcsr";
			reg = <0x1949000 0x100>;
			qcom,wifi_glb_cfg = <TCSR_WIFI_GLB_CFG>;
		};

		ess_tcsr@1953000 {
			compatible = "qcom,tcsr";
			reg = <0x1953000 0x1000>;
			qcom,ess-interface-select = <TCSR_ESS_PSGMII>;
		};

		tcsr@1957000 {
			compatible = "qcom,tcsr";
			reg = <0x1957000 0x100>;
			qcom,wifi_noc_memtype_m0_m2 = <TCSR_WIFI_NOC_MEMTYPE_M0_M2>;
		};

		usb2@60f8800 {
			status = "okay";
		};

		crypto@8e3a000 {
			status = "okay";
		};

		watchdog@b017000 {
			status = "okay";
		};

		ess-switch@c000000 {
			status = "okay";
		};

		edma@c080000 {
			status = "okay";
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&tlmm 0 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&tlmm 63 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};
};

&blsp1_spi1 {
	status = "okay";

	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";

	cs-gpios = <&tlmm 54 GPIO_ACTIVE_HIGH>,
		   <&tlmm 1 GPIO_ACTIVE_HIGH>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <24000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "bootloader";
				reg = <0x0 0x200000>;
				read-only;
			};
		};
	};

	flash@1 {
		compatible = "spinand,mt29f";
		reg = <1>;
		spi-max-frequency = <24000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "ubi";
				reg = <0x0 0x8000000>;
				read-only;
			};
		};
	};
};


&tlmm {
	i2c_0_pins: i2c_0_pinmux {
		pinmux {
			function = "blsp_i2c0";
			pins = "gpio58", "gpio59";
			drive-strength = <16>;
			bias-disable;
		};
	};

	spi_0_pins: spi_0_pinmux {
		mux {
			function = "blsp_spi0";
			pins = "gpio54", "gpio55", "gpio56", "gpio57";
			bias-disable;
		};
	};

	serial_pins: serial_pinmux {
		mux {
			pins = "gpio60", "gpio61";
			function = "blsp_uart0";
			bias-disable;
		};
	};
};

&cryptobam {
	status = "okay";
};

&wifi0 {
	status = "okay";
};

&wifi1 {
	status = "okay";
};

&usb2_hs_phy {
	/* Bluetooth module attached via USB */
	status = "okay";
};

&blsp1_i2c3 {
	pinctrl-0 = <&i2c_0_pins>;
	pinctrl-names = "default";
	status = "okay";

	led-controller@32 {
		/* 9-channel RGB LED controller */
		compatible = "national,lp5523";
		reg = <0x32>;
		clock-mode = [01];

		led_blue0: blue0 {
			chan-name = "blue0";
			label = "map-ac13000:blue:chan0";
			led-cur = [fa];
			max-cur = [ff];
		};

		blue1 {
			chan-name = "blue1";
			label = "map-ac13000:blue:chan1";
			led-cur = [fa];
			max-cur = [ff];
		};

		blue2 {
			chan-name = "blue2";
			label = "map-ac13000:blue:chan2";
			led-cur = [fa];
			max-cur = [ff];
		};

		led_green0: green0 {
			chan-name = "green0";
			label = "map-ac13000:green:chan0";
			led-cur = [fa];
			max-cur = [ff];
		};

		green1 {
			chan-name = "green1";
			label = "map-ac13000:green:chan1";
			led-cur = [fa];
			max-cur = [ff];
		};

		green2 {
			chan-name = "green2";
			label = "map-ac13000:green:chan2";
			led-cur = [fa];
			max-cur = [ff];
		};

		led_red0: red0 {
			chan-name = "red0";
			label = "map-ac13000:red:chan0";
			led-cur = [fa];
			max-cur = [ff];
		};

		red1 {
			chan-name = "red1";
			label = "map-ac13000:red:chan1";
			led-cur = [fa];
			max-cur = [ff];
		};

		red2 {
			chan-name = "red2";
			label = "map-ac13000:red:chan2";
			led-cur = [fa];
			max-cur = [ff];
		};
	};
};

&blsp1_uart1 {
	pinctrl-0 = <&serial_pins>;
	pinctrl-names = "default";
	status = "okay";
};
